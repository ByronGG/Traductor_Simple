/*
Algoritmo conpactado con la misma funcionalidad
*/
void rest() {
    while(true){
        if(lookahead == '+'){
            match('+');
            term();
            print('+');
            continue;
        }
        else if(lookahead == '-'){
            match('-');
            term();
            print('-');
            continue;
        }
        break;
    }
}